{
    "manifest_version": 3,
    "name": "Test",
    "description": "Testing Plugin",
    "version": "1.0",
    "content_scripts":[
        {
        "matches":["https://leetcode.com/*"],
        "js": ["content.js"]
        }
    ],
    "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["scripting"],
    "web_accessible_resources": [{
        "resources": ["script.js"],
        "matches": ["https://leetcode.com/*"]
    }]
}